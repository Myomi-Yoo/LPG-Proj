<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="game.LPG.soccerteam">
	<select id="teamcreate" parameterType="team">
		insert into team values(teamno_seq.nextval,#{teamName},null,#{teamLocation},null,#{teamAbility},null,null,null,#{teamPre},sysdate,#{teamAge},null,#{memberPrivate},#{teamGender})
	</select>
</mapper>